<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:DataGenerator/etc/dataProfileSchema.xsd">

    <entity name="fourthReview" type="review">
        <data key="nickname" unique="suffix">user</data>
        <data key="title">Product review</data>
        <data key="detail">New review product</data>
    </entity>
    <entity name="fifthReview" extends="fourthReview" type="review">
        <data key="nickname" unique="suffix">user</data>
    </entity>
    <entity name="sixthReview" extends="fourthReview" type="review">
        <data key="nickname" unique="suffix">user</data>
    </entity>
</entities>
