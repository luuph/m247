<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <bss_gallery>
            <general>
                <enable>1</enable>
                <album_title>Image Gallery</album_title>
                <image_per_page>20</image_per_page>
                <layout_type>standard</layout_type>
                <autoload>1</autoload>
                <page_speed>2000</page_speed>
                <fancybox>0</fancybox>
            </general>
        </bss_gallery>
    </default>
</config>
