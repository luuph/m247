<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument name="title" xsi:type="string" translate="true">Store Credit Information</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Bss\StoreCredit\Block\Customer\Account" name="bss_storecredit_customer_account" template="customer/account.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
