<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Meetanshi_ImageClean::imageclean" title="Image Clean">
                    <resource id="Meetanshi_ImageClean::imagecleanproductdata" title="Unused Product Images"/>
                    <resource id="Meetanshi_ImageClean::imageclean_use_product" title="Used Product Images"/>
                    <resource id="Meetanshi_ImageClean::imagecleancategorydata" title="Unused Category Images"/>
                    <resource id="Meetanshi_ImageClean::imageclean_configuration" title="Configuration"/>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Meetanshi_ImageClean::imageclean_config" title="Image Clean"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>